<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://www.gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />

	<inherits name='lando.systems.ld46.GdxDefinition' />
	<inherits name='aurelienribon.tweenengine' />
	<inherits name='regexodus' />

	<!-- Add classes to reflection cache here as needed: -->
	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld46.particles.Particle" />
	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld46.Audio" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.math.Rectangle" />

	<collapse-all-properties />
    
	<add-linker name="xsiframe"/>	
	<set-configuration-property name="devModeRedirectEnabled" value="true"/>
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>	
</module>
